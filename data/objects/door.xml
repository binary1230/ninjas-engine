<?xml version="1.0" encoding="ISO-8859-1"?>

<objectDefinitions>

<objectDef name="door" type="Door">
	<animations default="closed">
		<animation name="closed">
			<frames>
				<frame name="door1.png" duration="1" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>
		<animation name="opening">
			<frames>
				<frame type="sound" data="door3"/>
				<frame name="door1-open1.png" duration=4 />
				<frame name="door1-open2.png" duration=3 />
				<frame name="door1-open3.png" duration=3 />
				<frame name="door1-open4.png" duration=3 pause=1 />
			</frames>
			<show_in_order />
			<repeat />
		</animation>
	</animations>
	<properties>
		<noPhysicsRotate/>
		<solidObject/>
		<sensorOnly/>
	</properties>
	<sounds>
		<sound name="door3">sounds/door.wav</sound> <!-- REMEMBER, this will overwrite other sfx named door3 -DomPastSelf -->
	</sounds>
</objectDef>

</objectDefinitions>
